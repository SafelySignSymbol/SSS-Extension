{
  "manifest_version": 3,
  "name": "SSS Extension",
  "version": "3.0.0",
  "icons": {
    "16": "./sss16.png",
    "32": "./sss32.png",
    "64": "./sss64.png",
    "128": "./sss128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content_script.js"],
      "all_frames": true
    }
  ],
  "background": {
    "service_worker": "background_script.js"
  },
  "action": {
    "default_title": "SSS Extension",
    "default_icon": {
    "16": "./sss16.png",
    "32": "./sss32.png",
    "64": "./sss64.png",
    "128": "./sss128.png"
  }
  },
  "description": "Symbolブロックチェーンのトランザクションへと安全に署名を行うブラウザ拡張機能",
  "options_page": "options/options.html",
  "web_accessible_resources": [
    {
      "resources" : ["inject_script.js", "snackbar.css"],
      "matches" : ["*://*/*"]
    }
  ],
  "permissions": [
    "storage",
    "contextMenus",
    "tabs"
  ]
}